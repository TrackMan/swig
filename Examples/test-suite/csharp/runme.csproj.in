<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <AssemblyName>@TEST_NAME@_runme</AssemblyName>
    <RootNamespace>@TEST_NAME@_runme</RootNamespace>
    <AllowUnsafeBlocks>@ALLOW_UNSAFE@</AllowUnsafeBlocks>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
  </PropertyGroup>

  <ItemGroup>
    <!-- Include the runme file -->
    <Compile Include="@TEST_NAME@_runme.cs" />
  </ItemGroup>

  <ItemGroup>
    <!-- Reference the wrapper library project -->
    <ProjectReference Include="../@TEST_NAME@.csproj" />
  </ItemGroup>

</Project>

